<?xml version="1.0"?>

<PropertyList include="fokker100-base.xml">

 <sim>
  <description>Fokker 70 (JSBSim)</description>
  <author>Erik Hofman</author>
  <status>beta</status>
 
  <flight-model>jsb</flight-model>
  <aero>fokker70</aero>

  <model>
   <path>Aircraft/fokker100/Models/fokker70.xml</path>
  </model>

  <view n="0">
   <internal archive="y">true</internal>
   <config>
    <pitch-offset-deg>-6.5</pitch-offset-deg>
    <x-offset-m archive="y">-0.57</x-offset-m>	<!-- left -->
    <y-offset-m archive="y">1.9</y-offset-m>	<!-- up -->
    <z-offset-m archive="y">-10.45</z-offset-m>	<!-- fwd -->
   </config>
  </view>
 </sim>

 <nasal>
  <f70>
   <file>Aircraft/fokker100/Nasal/fokker.nas</file>
  </f70>
 </nasal>

</PropertyList>
