<?xml version="1.0"?>

<PropertyList include="fokker100-base.xml">

 <sim>
  <description>Fokker 70 (JSBSim)</description>
  <author>Erik Hofman</author>
  <status>beta</status>
 
  <flight-model>jsb</flight-model>
  <aero>fokker100</aero>

  <model>
   <path>Aircraft/fokker100/Models/fokker70.xml</path>
  </model>
 </sim>

 <nasal>
  <f70>
   <file>Aircraft/fokker100/Models/fokker.nas</file>
  </f70>
 </nasal>

 <input>
  <keyboard>
   <key n="66">
    <name>B</name>
    <desc>Toggle parking brake on or off</desc>
    <binding>
     <command>nasal</command>
     <script>f70.testDoor()</script>
     </binding>
   </key>
  </keyboard>
 </input>

</PropertyList>
