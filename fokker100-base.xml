<?xml version="1.0"?>

<PropertyList>

	<sim>

		<systems>
			<!-- O piloto automtico do fokker100 -->
			<autopilot>
				<path>Aircraft/fokker100/Systems/737-autopilotV5.xml</path>
			</autopilot>
			<electrical>
				<path>Aircraft/fokker100/Systems/electrical.xml</path>
			</electrical>
			<system>
				<path>Aircraft/Generic/generic-systems.xml</path>
			</system>
		</systems>


		<!-- Definies para splash screen -->
		<startup>
			<splash-texture>Aircraft/fokker100/splash.png</splash-texture>
		</startup>


		<fuel-fraction>0.7</fuel-fraction>

		<sound>
			<path>Aircraft/fokker100/Sounds/fokker100-sound.xml</path>
		</sound>

		<model>
			<path>Aircraft/fokker100/Models/fokker100.xml</path>
			<livery>
				<name type="string">TAM</name>
				<texture>TAM.rgb</texture>
			</livery>
			<fabric>
				<name type="string">Default</name>
				<texture>fabric.rgb</texture>
			</fabric>

		</model>

		<virtual-cockpit archive="y">true</virtual-cockpit>

		<chase-distance-m type="double" archive="y">-50.0</chase-distance-m>

		<!-- O menu de ajuda dessa aeronave -->
		<help>
			<title>Fokker 100</title>
			<line>Choose your favorit texture using the key above</line>
			<line>To start the plane you have to select "Engine Starter" and then when</line>
			<line>n2 shows 25 you have to select "Engine Start", If you want to cutoff</line>
			<line>the engine just select "Engine Start"</line>
			<key>
				<name>l</name>
				<desc>Brings up the Livery Switching Dialog</desc>
			</key>
			<key>
				<name>Del</name>
				<desc>Toggle Reversers</desc>
			</key>
			<key>
				<name>Ctrl+B</name>
				<desc>Toggle SpeedBrake</desc>
			</key>
			<key>
				<name>R</name>
				<desc>Cuttoff engine</desc>
			</key>
			<key>
				<name>S</name>
				<desc>toggle spoilers</desc>
			</key>

		</help>


		<menubar>
			<default>
				<menu n="5">

					<item n="10">
						<label>Pushback</label>
						<name>pushback</name>
						<binding>
							<command>dialog-show</command>
							<dialog-name>pushback</dialog-name>
						</binding>
					</item>

				</menu>

				<menu n="10">
					<label>Fokker 100</label>
					<enabled type="bool">true</enabled>


					<item>
						<label>Select Livery</label>
						<binding>
							<command>nasal</command>
							<script>aircraft.livery.dialog.open()</script>
						</binding>
					</item>
					<item>
						<label>Pushback</label>
						<name>pushback</name>
						<binding>
							<command>dialog-show</command>
							<dialog-name>pushback</dialog-name>
						</binding>
					</item>
				</menu>
			</default>
		</menubar>



	</sim>


	<!-- Engines using thrust reversal. http://wiki.flightgear.org/index.php/Howto:_Add_thrust_reversal -->
	<!-- Prestes Hangar code - SET-2009 -->
	<engines>
		<engine n="0">
			<running>true</running>
			<reverser-position type="double">0.000000</reverser-position>
		</engine>
		<engine n="1">
			<running>true</running>
			<reverser-position type="double">0.000000</reverser-position>
		</engine>
	</engines>



	<input>
		<!-- set 25% flap adjustment with each detent -->
		<keyboard>


			<key n="82">
				<name>R</name>
				<desc>Toggle cutoff on Selected Engine(s)</desc>
				<binding>
					<condition>
						<property>/sim/input/selected/engine[0]</property>
					</condition>
					<command>property-toggle</command>
					<property>/controls/engines/engine[0]/cutoff</property>
				</binding>
				<binding>
					<condition>
						<property>/sim/input/selected/engine[1]</property>
					</condition>
					<command>property-toggle</command>
					<property>/controls/engines/engine[1]/cutoff</property>
				</binding>

			</key>
			<key n="83">
				<name>S</name>
				<desc>Fire Starter on Selected Engine(s)</desc>
				<binding>
					<condition>
						<property>/sim/input/selected/engine[0]</property>
					</condition>
					<command>property-assign</command>
					<property>/controls/engines/engine[0]/starter</property>
					<value type="bool">true</value>
				</binding>
				<binding>
					<condition>
						<property>/sim/input/selected/engine[1]</property>
					</condition>
					<command>property-assign</command>
					<property>/controls/engines/engine[1]/starter</property>
					<value type="bool">true</value>
				</binding>
			</key>

			<key n="127">
				<name>Delete</name>
				<desc>Toggle Reversers</desc>
				<binding>
					<command>nasal</command>
					<script>reversethrust.togglereverser()</script>
				</binding>
			</key>


			<key n="70">

				<name>F</name>

				<desc>Spoilers</desc>

				<binding>

					<command>property-toggle</command>

					<property>/controls/flight/spoilers</property>

				</binding>

			</key>


			<!-- set 12.5% flap adjustment with each detent -->

			<key n="91">

				<name>[</name>

				<desc>Decrease flaps.</desc>

				<binding>

					<command>property-adjust</command>

					<property>/controls/flight/flaps</property>

					<step type="double">-0.125</step>

				</binding>

			</key>

			<key n="93">

				<name>]</name>

				<desc>Increase flaps.</desc>

				<binding>

					<command>property-adjust</command>

					<property>/controls/flight/flaps</property>

					<step type="double">0.125</step>

				</binding>

			</key>

			<key n="9">
				<name>Tab</name>
				<desc>switch autobrake</desc>
				<binding>
					<command>property-cycle</command>
					<property>/controls/gear/autobrakes</property>
					<value>0</value>
					<value>1</value>
					<value>2</value>
					<value>3</value>
					<value>4</value>
					<value>5</value>
				</binding>
			</key>

		</keyboard>
	</input>


	<nasal>
		<reversethrust>
			<file>Aircraft/fokker100/Nasal/reverse-thrust.nas</file>
		</reversethrust>
		<tyresmoke>
			<file>Aircraft/fokker100/Nasal/tyresmoke.nas</file>
		</tyresmoke>
		<livery>
			<file>Aircraft/fokker100/Nasal/livery.nas</file>
		</livery>
		<airground>
			<file>Aircraft/fokker100/Nasal/air-ground.nas</file>
		</airground>

	</nasal>




	<consumables>
		<fuel>
			<!-- 13365 litres -->
			<tank n="0">
				<level-gal_us archive="y">2070</level-gal_us>
			</tank>
			<tank n="1">
				<level-gal_us archive="y">2070</level-gal_us>
			</tank>
		</fuel>
	</consumables>

	<controls>
		<gear>
			<autobrakes type="int">0</autobrakes>
			<!-- 0=off, 1=1, 2=2, 3=3, 4=MAX, 5=RTO -->
		</gear>
		<flight>
			<autospeedbrakes-armed type="string">false</autospeedbrakes-armed>
		</flight>

		<switches>
			<landing-light-l type="bool">true</landing-light-l>
			<landing-light-r type="bool">true</landing-light-r>
			<taxi-light>0</taxi-light>
			<inboard-lights>0</inboard-lights>
			<outboard-lights>0</outboard-lights>
			<nav-lights>0</nav-lights>
			<beacon>1</beacon>
			<strobe>1</strobe>
			<logo-lights>1</logo-lights>
			<cabin-lights>0</cabin-lights>
			<map-lights>0</map-lights>
			<master-panel>0</master-panel>
			<instruments-lights>0</instruments-lights>
			<seatbelt-sign>0</seatbelt-sign>
		</switches>
	</controls>

	<gear>
		<gear n="0">
			<tyre-smoke type="int">0</tyre-smoke>
			<sprayspeed-ms type="double">0</sprayspeed-ms>
			<spray-density type="double">0</spray-density>
			<caster-angle-deg-damped type="double">0</caster-angle-deg-damped>
		</gear>
		<gear n="1">
			<tyre-smoke type="int">0</tyre-smoke>
			<sprayspeed-ms type="double">0</sprayspeed-ms>
			<spray-density type="double">0</spray-density>
		</gear>
		<gear n="2">
			<tyre-smoke type="int">0</tyre-smoke>
			<sprayspeed-ms type="double">0</sprayspeed-ms>
			<spray-density type="double">0</spray-density>
		</gear>
		<!--<tailhook>
			<position-norm type="double">0</position-norm>
		</tailhook>-->
	</gear>

	<instrumentation>
		<mk-viii>
			<serviceable>true</serviceable>
			<configuration-module>
				<category-1>1</category-1>
				<!-- GA Fast, 150' Mode 4B -->
			</configuration-module>
			<inputs>
				<arinc429>
					<decision-height-ncd>false</decision-height-ncd>
					<decision-height>200</decision-height>
				</arinc429>
			</inputs>
		</mk-viii>
		<wxradar>
			<serviceable>true</serviceable>
			<range type="int" archive="y">20</range>
			<minimized type="bool" archive="y">false</minimized>
			<switch archive="y">off</switch>
			<mode archive="y">wx</mode>
			<lightning type="bool" archive="y">false</lightning>
			<display-mode archive="y">arc</display-mode>
		</wxradar>
		<radar>
			<range type="int" archive="y">20</range>
		</radar>
		<turn-indicator>
			<serviceable type="bool">true</serviceable>
			<spin type="double">1</spin>
		</turn-indicator>
	</instrumentation>



</PropertyList>
