			<entry><ind>1292</ind><dep>-0.435</dep></entry>
			<entry><ind>1472</ind><dep>-0.498</dep></entry>
		</interpolation>
		<axis>
			<x>0</x>
			<y>1</y>
		</axis>
	</animation>

<!-- lighting -->

