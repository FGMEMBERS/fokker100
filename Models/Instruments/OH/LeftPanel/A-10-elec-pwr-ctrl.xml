<?xml version="1.0"?>

<PropertyList>

 <path>A-10-elec-pwr-ctrl.ac</path>

<!-- Lighting -->
	<animation> 
		<object-name>mount</object-name>
		<type>material</type>
		<emission> 
			<factor-prop>controls/lighting/panel-norm</factor-prop>
			<red>0.45</red>
			<green>0.34</green>
			<blue>0.18</blue>
		</emission>
	</animation>

	<animation> 
		<object-name>switches</object-name>
		<type>material</type>
		<emission> 
			<factor-prop>controls/lighting/panel-norm</factor-prop>
			<red>0.45</red>
			<green>0.34</green>
			<blue>0.18</blue>
		</emission>
	</animation>

	<animation>
		<object-name>elec-pwr-panel</object-name>
		<type>noshadow</type>
	</animation>


	<animation>
		<type>range</type>
		<min-m>0</min-m>
		<max-m>15</max-m>
	</animation>

</PropertyList>
