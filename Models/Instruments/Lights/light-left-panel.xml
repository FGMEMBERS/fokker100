<?xml version="1.0"?>

	<PropertyList>

	<path>light-left-panel.ac</path>

	<!-- Beacon -->
	
    <animation>
		<object-name>tail-switch</object-name>
		<type>rotate</type>
		<property>controls/lighting/logo-lights</property>
		<interpolation>
            <entry><ind>0</ind><dep>0</dep></entry>
			<entry><ind>1</ind><dep>50</dep></entry>
        </interpolation>
		<center>
			<x-m>-0.0549</x-m>
			<y-m>-0.0429</y-m>
			<z-m>-0.0047</z-m>
		</center>
		<axis>
			<x>0</x>
			<y>-1</y>
			<z>0</z>
		</axis>
	</animation>
    <animation>
		<type>pick</type>
		<visible>true</visible>
		<object-name>tail-switch</object-name>
		<action>
			<button>0</button>
			<repeatable>false</repeatable>
			<binding>
				<command>property-toggle</command>
				<property>controls/lighting/logo-lights</property>
			</binding>
		</action>
	</animation>

