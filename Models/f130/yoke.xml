<?xml version="1.0"?>

<PropertyList>

  <path>yoke.ac</path>

  <animation>
    <!-- Objets opaques -->
    <object-name>yoke</object-name>
  </animation>

  <animation>
  <type>rotate</type>
  <object-name>focc</object-name>
  <object-name>clipboard</object-name>
  <property>surface-positions/elevator-pos-norm</property>
  <factor>-10</factor>
  <center>
   <x-m>-0.01</x-m>
   <y-m>0.0</y-m>
   <z-m>0.0</z-m>
  </center>
  <axis>
   <x>0.000000</x>
   <y>1.000000</y>
   <z>0.000000</z>
  </axis>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>fowheel</object-name>
  <!-- object-name>clipboard</object-name -->
  <property>surface-positions/left-aileron-pos-norm</property>
  <factor>-60</factor>
  <center>
   <x-m>0.0</x-m>
   <y-m>0.0</y-m>
   <z-m>0.765</z-m>
  </center>
  <axis>
   <x>1.000000</x>
   <y>0.000000</y>
   <z>0.000000</z>
  </axis>
 </animation>

</PropertyList>

