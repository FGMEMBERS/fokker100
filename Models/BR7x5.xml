<?xml version="1.0"?>

<PropertyList>

 <path>BR7x5.ac</path>

 <animation>
  <type>material</type>
  <object-name>rotating_fan_disk</object-name>
  <object-name>fan_disk</object-name>
  <object-name>upper_thrust_reverse</object-name>
  <object-name>lower_thrust_reverse</object-name>
  <property-base>sim/model/livery</property-base>
  <texture-prop>texture</texture-prop>
  <texture>fokker100.rgb</texture>
 </animation>

   <!-- reverser -->
 <animation>
  <type>rotate</type>
  <object-name>upper_thrust_reverse</object-name>
  <property>engines/engine[0]/reverser-pos-norm</property>
  <factor>-60</factor>
  <center>
   <x-m>11.040000</x-m>
   <y-m>-0.250000</y-m>
   <z-m>1.240000</z-m>
  </center>
  <axis>
   <x>0.000000</x>
   <y>-1.000000</y>
   <z>0.000000</z>
  </axis>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>lower_thrust_reverse</object-name>
  <property>engines/engine[0]/reverser-pos-norm</property>
  <factor>-60</factor>
  <center>
   <x-m>11.040000</x-m>
   <y-m>0.250000</y-m>
   <z-m>1.240000</z-m>
  </center>
  <axis>
   <x>0.000000</x>
   <y>1.000000</y>
   <z>0.000000</z>
  </axis>
 </animation>

 <animation>
  <object-name>rotating_fan_disk</object-name>
  <object-name>fan_disk</object-name>
 </animation>

 <animation>
  <type>spin</type>
  <object-name>fan_disk</object-name>
  <property>engines/engine[0]/n1</property>
  <factor>67</factor>
  <center>
   <x-m>0.00000</x-m>
   <y-m>2.60000</y-m>
   <z-m>1.30000</z-m>
  </center>
  <axis>
   <x>1.000000</x>
   <y>0.000000</y>
   <z>0.000000</z>
  </axis>
 </animation>

 <animation>
  <type>spin</type>
  <object-name>rotating_fan_disk</object-name>
  <property>engines/engine[0]/n1</property>
  <factor>67</factor>
  <center>
   <x-m>0.00000</x-m>
   <y-m>2.60000</y-m>
   <z-m>1.30000</z-m>
  </center>
  <axis>
   <x>1.000000</x>
   <y>0.000000</y>
   <z>0.000000</z>
  </axis>
 </animation>

 <animation>
  <type>blend</type>
  <object-name>fan_disk</object-name>
  <property>engines/engine[0]/n1</property>
  <factor>067</factor>
 </animation>

 <model>
  <path>Aircraft/fokker100/Models/Effects/trail/trailL.xml</path>
  <name>Left Jet Contrail</name>
  <offsets>
   <x-m> 11.00 </x-m>
   <y-m> -2.6 </y-m>
   <z-m> 1.50 </z-m>
  </offsets>
 </model>

 <model>
  <path>Aircraft/fokker100/Models/Effects/enginefire.xml</path>
  <condition>
   <or>
    <property>sim/crashed</property>
    <property>controls/engines/engine[0]/on-fire</property>
   </or>
  </condition>
  <offsets>
   <x-m> 12.00 </x-m>
   <y-m> 2.6 </y-m>
   <z-m> 1.50 </z-m>
  </offsets>
 </model>

</PropertyList>

