            <max-dist>2.0</max-dist>
            <mode>once</mode>
            <path>Aircraft/fokker100/Sounds/altitude-10.wav</path>
            <condition>
				<less-than-equals>
                    		<property>position/altitude-agl-ft</property>
                    		<value>10</value>
                  	</less-than-equals>
            </condition>
            <volume>
                <property>sim/sound/Evolume</property>
